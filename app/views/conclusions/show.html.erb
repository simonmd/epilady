<%- model_class = Conclusion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:observaciones) %>:</strong></dt>
  <dd><%= @conclusion.observaciones %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rm) %>:</strong></dt>
  <dd><%= @conclusion.rm %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hemisferio) %>:</strong></dt>
  <dd><%= @conclusion.hemisferio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sospecha) %>:</strong></dt>
  <dd><%= @conclusion.sospecha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:correlacion) %>:</strong></dt>
  <dd><%= @conclusion.correlacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:report_id) %>:</strong></dt>
  <dd><%= @conclusion.report_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              conclusions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_conclusion_path(@conclusion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              conclusion_path(@conclusion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
