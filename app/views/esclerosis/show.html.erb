<%- model_class = Esclerosi -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:atrofia_sunj) %>:</strong></dt>
  <dd><%= @esclerosi.atrofia_sunj %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hiperintensidad) %>:</strong></dt>
  <dd><%= @esclerosi.hiperintensidad %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estructura_anormal) %>:</strong></dt>
  <dd><%= @esclerosi.estructura_anormal %></dd>
  <dt><strong><%= model_class.human_attribute_name(:volumen) %>:</strong></dt>
  <dd><%= @esclerosi.volumen %></dd>
  <dt><strong><%= model_class.human_attribute_name(:esclerosis) %>:</strong></dt>
  <dd><%= @esclerosi.esclerosis %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hallazgos_extra) %>:</strong></dt>
  <dd><%= @esclerosi.hallazgos_extra %></dd>
  <dt><strong><%= model_class.human_attribute_name(:report_id) %>:</strong></dt>
  <dd><%= @esclerosi.report_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              esclerosis_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_esclerosi_path(@esclerosi), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              esclerosi_path(@esclerosi),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
